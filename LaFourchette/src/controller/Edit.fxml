<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="leftmain" prefHeight="576.0" prefWidth="383.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.EditController">
   <children>
      <BorderPane fx:id="BorderPane" prefHeight="580.0" prefWidth="383.0">
         <center>
            <Pane minWidth="-Infinity" prefHeight="178.0" prefWidth="197.0" styleClass="button-background" stylesheets="@../CSS/AllStyle.css" BorderPane.alignment="CENTER">
               <children>
                  <ImageView fx:id="imageviewedit" fitHeight="129.0" fitWidth="197.0" layoutX="96.0" layoutY="261.0" pickOnBounds="true" preserveRatio="true" />
                  <Button fx:id="insertimagebutton" layoutX="93.0" layoutY="390.0" mnemonicParsing="false" onAction="#insertImage" prefHeight="35.0" prefWidth="203.0" styleClass="button1" stylesheets="@../CSS/AllStyle.css" text="Insert" textFill="#edeee9" />
                  <VBox alignment="TOP_CENTER" layoutX="70.0" layoutY="40.0" minWidth="-Infinity" opacity="0.33" prefHeight="162.0" prefWidth="243.0">
                     <children>
                        <TextField fx:id="tfreferenceedit" prefHeight="64.0" prefWidth="305.0" promptText="Nom">
                           <font>
                              <Font name="Bernard MT Condensed" size="16.0" />
                           </font>
                        </TextField>
                        <TextField fx:id="tfdesignationedit" promptText="Designation" VBox.vgrow="ALWAYS" />
                        <TextField fx:id="tfprixedit" promptText="Prix" />
                        <TextField fx:id="tfproduitsedit" prefHeight="31.0" prefWidth="345.0" promptText="Produits" />
                        <TextField fx:id="tfdescriptionedit" promptText="Description" />
                     </children>
                  </VBox>
                  <Label fx:id="imagepath" layoutX="96.0" layoutY="428.0" prefHeight="72.0" prefWidth="120.0" text="Path:" />
               </children>
            </Pane>
         </center></BorderPane>
      <Button fx:id="editbutton" defaultButton="true" layoutX="44.0" layoutY="538.0" onAction="#modifierFormateur" prefHeight="15.0" prefWidth="91.0" text="_Apply">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button cancelButton="true" layoutX="156.0" layoutY="538.0" onAction="#onCancel" prefHeight="15.0" prefWidth="91.0" text="_Cancel">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
